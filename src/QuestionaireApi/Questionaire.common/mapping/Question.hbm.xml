<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Questionaire.common" 
                   namespace="Questionaire.common.model" >
  <class name="Question">
    <id name="ID">
      <generator class="identity"/>
    </id>
    <property name="QuestionnaireID"/>
    <property name="Label"/>
    <property name="Type"/>
    <property name="ChoiceGroupID"/>
    <property name="IsMultiAnswer"/>
    <property name="DisplayOrder"/>
    <property name="CreatedAt"/>
    <property name="UpdatedAt"/>
  </class>
</hibernate-mapping>
